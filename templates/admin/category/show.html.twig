	{{ include('admin/admin/adminHead.html.twig') }}


	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ category.id }}</td>
			</tr>
			<tr>
				<th>Parentid</th>
				<td>{{ category.parentid }}</td>
			</tr>
			<tr>
				<th>Title</th>
